{
  "name": "featdd/dpn-glossary",
  "type": "typo3-cms-extension",
  "description": "Glossary extension with page parser",
  "homepage": "https://featdd.de",
  "license": [
    "GPL-2.0-or-later"
  ],
  "require": {
    "typo3/cms-core": "~9.5.0",
    "php": "^7.2",
    "ext-dom": "*",
    "ext-libxml": "*",
    "require ivopetkov/html5-dom-document-php": "^2"
  },
  "autoload": {
    "psr-4": {
      "Featdd\\DpnGlossary\\": "Classes"
    }
  },
  "replace": {
    "dpn_glossary": "self.version",
    "typo3-ter/dpn-glossary": "self.version"
  }
}
