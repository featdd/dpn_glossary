{
  "name": "featdd/dpn-glossary",
  "type": "typo3-cms-extension",
  "description": "Glossary extension with page parser",
  "homepage": "https://extensions.typo3.org/extension/dpn_glossary",
  "license": [
    "GPL-2.0-or-later"
  ],
  "require": {
    "typo3/cms-core": "^12.4||^13.4",
    "php": ">=8.1 <8.4",
    "ext-dom": "*",
    "ext-libxml": "*",
    "ext-pdo": "*"
  },
  "suggest": {
    "ext-intl": "Used for transliterate characters of terms for grouped list view"
  },
  "autoload": {
    "psr-4": {
      "Featdd\\DpnGlossary\\": "Classes"
    }
  },
  "replace": {
    "typo3-ter/dpn-glossary": "self.version"
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "dpn_glossary"
    }
  }
}
